	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:local="clr-namespace:MSGraph.Mobile.Views">

	<MasterDetailPage.Master>
		<local:MenuPage />
	</MasterDetailPage.Master>

	<MasterDetailPage.Detail>
		<NavigationPage>
			<x:Arguments>
				<local:WelcomePage />
			</x:Arguments>
		</NavigationPage>
	</MasterDetailPage.Detail>
</MasterDetailPage>